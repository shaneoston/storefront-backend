{
  "name": "storefront-backend",
  "version": "1.0.0-beta",
  "main": "src/index.ts",
  "engines": {
    "node": "16.13.0"
  },
  "repository": "git@github.com:shaneoston/storefront-backend.git",
  "author": "Shane Oston Stowe <shane@keepleft.dev>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.3",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/db-migrate-pg": "^0.0.10",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.3",
    "@types/node": "^17.0.21",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.11",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "jasmine": "^4.0.2",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "scripts": {
    "build": "yarn prebuild && npx tsc",
    "create-dev-db": "db-migrate --config database_create.json db:create full_stack_dev",
    "create-test-db": "db-migrate --config database_create.json db:create full_stack_test",
    "prebuild": "rimraf dist",
    "reset-test-db": "db-migrate reset --e test",
    "reset-dev-db": "db-migrate reset --e dev",
    "start": "nodemon src/index.ts",
    "test": "db-migrate up -e test && yarn build && NODE_ENV=test jasmine && db-migrate reset --e test"
  }
}
